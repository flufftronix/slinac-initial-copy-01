a,
button,
.text-link {
  color: inherit;
  text-decoration: underline 2px solid;
  text-decoration-color: #2F6EA1;
  text-decoration-skip: ink;
  outline: none;
  box-shadow: none;
  border: none; }
  a:focus, a:hover,
  button:focus,
  button:hover,
  .text-link:focus,
  .text-link:hover {
    color: #2F6EA1;
    text-decoration: underline;
    outline: none;
    box-shadow: none;
    border: none; }
  a:not(.logo):not(.nav-link),
  button:not(.logo):not(.nav-link),
  .text-link:not(.logo):not(.nav-link) {
    font-weight: inherit;
    position: relative; }
    a:not(.logo):not(.nav-link).passive:after,
    button:not(.logo):not(.nav-link).passive:after,
    .text-link:not(.logo):not(.nav-link).passive:after {
      opacity: 1; }
    a:not(.logo):not(.nav-link).passive:hover:after,
    button:not(.logo):not(.nav-link).passive:hover:after,
    .text-link:not(.logo):not(.nav-link).passive:hover:after {
      opacity: 1; }
    a:not(.logo):not(.nav-link):after,
    button:not(.logo):not(.nav-link):after,
    .text-link:not(.logo):not(.nav-link):after {
      content: '';
      position: absolute;
      background: #2F6EA1;
      width: 100%;
      bottom: 0;
      left: 0;
      opacity: .5;
      z-index: -1; }
    a:not(.logo):not(.nav-link):hover:after, a:not(.logo):not(.nav-link):focus:after,
    button:not(.logo):not(.nav-link):hover:after,
    button:not(.logo):not(.nav-link):focus:after,
    .text-link:not(.logo):not(.nav-link):hover:after,
    .text-link:not(.logo):not(.nav-link):focus:after {
      bottom: 0;
