/* Accordion styles */
.tab {
  width: 100%;
  color: white;
  overflow: hidden;
  text-align: justify; }
  .tab p {
    margin: 0 2rem; }
  .tab .sectionheader {
    text-transform: uppercase;
    font-size: 1.5rem;
    color: #134663; }
  .tab blockquote {
    background-color: rgba(255, 255, 255, 0.3);
    border-radius: .5rem;
    margin: 1rem;
    border: 0;
    padding: 1.5rem 1.5rem .5rem; }
    .tab blockquote p {
      margin: 0 0 1rem 0; }
  .tab-label {
    display: flex;
    justify-content: space-between;
    padding: 1em;
    background: #134663;
    font-weight: bold;
    cursor: pointer;
    /* Icon */ }
    .tab-label:hover {
      background: #23537a; }
    .tab-label::after {
      content: "\276F";
      width: 1em;
      height: 1em;
      text-align: center;
      transition: all .35s; }
  .tab-content {
    max-height: 0;
    padding: 0 1em;
    color: #000;
    background: rgba(25, 96, 151, 0.4);
    transition: all .35s; }
  .tab-close {
    display: flex;
    justify-content: flex-end;
    padding: 1em;
    font-size: 0.75em;
    background: #2F6EA1;
    cursor: pointer; }
    .tab-close:hover {
      background: #23537a; }

input:checked + .tab-label {
  background: #23537a; }
  input:checked + .tab-label::after {
    transform: rotate(90deg); }

input:checked ~ .tab-content {
  max-height: 1000vh;
  padding: 1em; }

